  "description": "Discord bot to automatically mute members not in the channel's designated game",
  "engines": {
    "node": ">=12 <15"
  },
  "main": "src/main.ts",
  "devDependencies": {
    "@types/node": "~14.0.23",
    "@typescript-eslint/eslint-plugin": "~3.2.0",
