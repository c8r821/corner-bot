    "eslint": "~7.2.0",
    "eslint-config-prettier": "~6.11.0",
    "nodemon": "^2.0.4",
    "prettier": "~2.0.5",
    "rimraf": "~3.0.2",
    "ts-node": "^8.10.2",
    "tsutils": "~3.17.0",
